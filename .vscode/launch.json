{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Extensions",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode"
            ],
            "sourceMaps": true,
            "smartStep": true,
            "outFiles": [
                "${workspaceFolder}/packages/*/dist/*.js"
            ]
        },
    ]
}