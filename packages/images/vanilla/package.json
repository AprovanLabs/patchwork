{
  "name": "@aprovan/patchwork-vanilla",
  "version": "0.1.0",
  "description": "Patchwork image: Plain browser image",
  "type": "module",
  "main": "./dist/setup.js",
  "types": "./dist/setup.d.ts",
  "exports": {
    ".": {
      "types": "./dist/setup.d.ts",
      "import": "./dist/setup.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "patchwork": {
    "platform": "browser",
    "esbuild": {
      "target": "es2020",
      "format": "esm",
      "jsx": "automatic"
    }
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
